<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app id="WebApp_ID">
	<display-name>Cookies</display-name>
	<servlet>
		<servlet-name>Cooky</servlet-name>
		<display-name>Cooky</display-name>
		<description></description>
		<servlet-class>cookies.Cooky</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Cooky2</servlet-name>
		<display-name>Cooky2</display-name>
		<description></description>
		<servlet-class>cookies.Cooky2</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Cooky3</servlet-name>
		<display-name>Cooky3</display-name>
		<description></description>
		<servlet-class>cookies.Cooky3</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Cooky</servlet-name>
		<url-pattern>/Cooky</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Cooky2</servlet-name>
		<url-pattern>/Cooky2</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Cooky3</servlet-name>
		<url-pattern>/Cooky3</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
</web-app>
